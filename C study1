#include <stdio.h>

int main()
{
    char *p[5]={"red", "blue", "yellow", "green", "black" };
    int i;
    char arr[10];
    scanf("%s",&arr);
    for(i=0;i<5;i++)
    {
        if(strcmp(*(p+i),arr)==0)
        {printf("%d",i+1);
        return 0;}
    }
    if(i==5)
        printf("Not Found");
    return 0;
}
